<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="SettingsText" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:textColor">@color/text_dark</item>
    </style>

    <style name="TabDark" parent="Widget.Design.TabLayout">
        <item name="tabSelectedTextColor">@color/text_dark</item>
        <item name="tabTextAppearance">@style/TabTextDark</item>
    </style>

    <style name="TabTextDark" parent="TextAppearance.Design.Tab">
        <item name="android:textColor">@color/text_dark</item>
    </style>

</resources>