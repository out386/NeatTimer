<?xml version="1.0" encoding="utf-8"?>
<gh.out386.timer.customviews.PrefsColourLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="false"
    android:orientation="vertical">

    <gh.out386.timer.customviews.PrefsColourTabLayout
        android:id="@+id/bottomPagerTab"
        style="@style/TabDark"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <android.support.v4.view.ViewPager
        android:id="@+id/bottomPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</gh.out386.timer.customviews.PrefsColourLinearLayout>